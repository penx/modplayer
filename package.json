{
  "name": "modplayer",
  "version": "0.0.1",
  "description": "MOD file player/tracker",
  "module": "index.js",
  "main": "cjs/index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest --coverage --verbose --passWithNoTests && npm run lint",
    "jest": "jest --verbose --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "npm run build:ecm && npm run build:commonjs",
    "build:ecm": "tsc -p tsconfig.prod.json",
    "build:commonjs": "tsc -p tsconfig.commonjs.json",
    "lint": "eslint '**/*.ts'"
  },
  "author": "Alasdair McLeay",
  "contributors": [
    "Noora Halme"
  ],
  "license": "MIT",
  "keywords": [
    "mod",
    "amiga",
    "music",
    "tracker",
    "player"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/penx/modplayer.git"
  },
  "bugs": {
    "url": "https://github.com/penx/modplayer/issues"
  },
  "homepage": "https://github.com/penx/modplayer#readme",
  "devDependencies": {
    "@jest/types": "^29.3.1",
    "@types/jest": "^27.4.1",
    "eslint": "^8.13.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0"
  }
}
